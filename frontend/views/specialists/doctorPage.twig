<div data-page-type="doctor_page">

  <div class="content_block doctor_preview">

    <div class="doctor_preview_bg">

      <a class="breadcrumbs _menu_link" href="/">breadcrumbs</a>
  
      <div class="doctor_preview_container">
    
        <div class="doctor_image" style="background: url({{doc.doctor_image}});"></div>
  
        <div class="doctor_preview_wrapper">
  
          <h2>{{doc.doctor_title}}</h1>
      
          <div class="specialities_wrapper">
      
            {% for spec in doc.medicalSpecialties%}
      
              <a class="_menu_link" href="/speciality/{{spec.alias}}">{{spec.menu_title}}</a>
      
            {% endfor %}
      
          </div>
  
          <div class="swiper_container clinics_wrapper">
  
            <div class="swiper-wrapper">
        
              <div class="swiper-slide clinic_item">

                <a class="_menu_link_no_borders" href="/">

                  <p class="clinic_name">Центр Эстетической Стоматологии</p>
                  <p class="clinic_address">пр-кт Вернадского, д. 11/19<br>• м. Университет</p>
                  <p class="clinic_phone">Тел.: 8 (495) 600 47 45</p>
                  
                </a>

              </div>
        
              <div class="swiper-slide clinic_item">

                <a class="_menu_link_no_borders" href="/">

                  <p class="clinic_name">Центр Эстетической Стоматологии</p>
                  <p class="clinic_address">пр-кт Вернадского, д. 11/19<br>• м. Университет</p>
                  <p class="clinic_phone">Тел.: 8 (495) 600 47 45</p>
                  
                </a>

              </div>
        
              <div class="swiper-slide clinic_item">

                <a class="_menu_link_no_borders" href="/">

                  <p class="clinic_name">Центр Эстетической Стоматологии</p>
                  <p class="clinic_address">пр-кт Вернадского, д. 11/19<br>• м. Университет</p>
                  <p class="clinic_phone">Тел.: 8 (495) 600 47 45</p>
                  
                </a>

              </div>
        
            </div>

            <div class="revolver_navigation_wrapper">

              <div class="swiper-button-prev revolver_navigation"></div>
              <div class="swiper-button-next revolver_navigation"></div>
      
            </div>
  
          </div>
      
          <button class="_button">Записаться на приём</button>
  
        </div>
    
      </div>

    </div>


  </div>

